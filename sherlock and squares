#include <bits/stdc++.h>
using namespace std;
int main()
{
    int q;
    cin>>q;
    for(int i=0;i<q;i++)
    {
        int a,b;
      cin>>a>>b;
      int count=0;
      for(int j=0; ;j++) 
      {
        int square=j*j;
        if(square<=b && square>=a)
        {
            count++;
        }
        else if(square>b)
        {
            break;
        }
      }
      cout<<count<<endl; 
    }
    return 0;
}
